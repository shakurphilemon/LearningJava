class OuterClass {
    int x = 25;

    class InnerClass {
        int y = 70;
    }
}
class Main {
    public static void main(String[] args) {
        OuterClass myOuterClass = new OuterClass();
        OuterClass.InnerClass myInnerClass =  myOuterClass.new InnerClass();
        System.out.println(myInnerClass.y *  myOuterClass.x);
    }
}
