abstract  class Animal {
    public abstract void animalSound();
    public void Sleep() {
        System.out.println("Print: ZZZ ZZZ ");
    }
}
class Cat extends Animal {
    public void animalSound(){
        System.out.println("Print Sound: Meow");
    }
}
class Dog extends Animal {
    public void animalSound(){
        System.out.println("Print: WOO WOO");
    }
}

class Main {
    public static void main(String[] args) {
        Cat myCat = new Cat();
        Dog myDog  = new Dog();
        myCat.animalSound();
        myDog.animalSound();
        myCat.Sleep();
        myDog.Sleep();

    }
}
